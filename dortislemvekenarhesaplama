public class Main {
    void main() {
        int sayi1 = 6;
        int sayi2 = 3;
        int carpim = sayi1 * sayi2;
        int toplam = sayi1 + sayi2;
        int fark = sayi1 - sayi2;
        int bolum = sayi1 / sayi2;
        system.out.println(carpim, toplam, fark, bolum);
        int kenar = 6;
        int cevre = kenar*3;
        int alan = (kenar*kenar)/2;
        system.out.println(cevre , alan);
    }
}

